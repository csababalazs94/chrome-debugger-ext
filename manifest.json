{
  "name": "Start Debugger Extension",
  "description": "Starts a debugger from console, with after a 3s delay",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "Start Debugger",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
